@model SGCP.Web.Models.ModuloProducto.ProductoGetModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del Producto</h1>

<div>
    <hr />
    <dl class="row">

        <dt class = "col-sm-2">
            IdProducto
        </dt>
        <dd class = "col-sm-10">
            @Model.idProducto
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Model.nombre
        </dd>
        <dt class = "col-sm-2">
            Descripcion
        </dt>
        <dd class = "col-sm-10">
            @Model.descripcion
        </dd>
        <dt class = "col-sm-2">
            Categoria
        </dt>
        <dd class = "col-sm-10">
            @Model.categoria
        </dd>
        <dt class = "col-sm-2">
            Precio
        </dt>
        <dd class = "col-sm-10">
            @Model.precio
        </dd>
        <dt class = "col-sm-2">
            Stock
        </dt>
        <dd class = "col-sm-10">
            @Model.stock
        </dd>
        <dt class="col-sm-2">
            FechaCreacion
        </dt>
        <dd class="col-sm-10">
            @Model.fechaCreacion
        </dd>
        <dt class="col-sm-2">
            FechaModificacion
        </dt>
        <dd class="col-sm-10">
            @Model.fechaModificacion
        </dd>
        <dt class="col-sm-2">
            UsuarioModificacion
        </dt>
        <dd class="col-sm-10">
            @Model.usuarioModificacion
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { id = Model.idProducto }) |
    <a asp-action="Index">Back to List</a>
</div>
