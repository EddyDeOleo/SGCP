@model SGCP.Web.Models.ModuloUsuarios.ClienteModels.ClienteGetModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del Cliente</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ClienteId
        </dt>
        <dd class="col-sm-10">
            @Model.clienteId
        </dd>
        <dt class="col-sm-2">
            Nombre
        </dt>
        <dd class="col-sm-10">
            @Model.nombre
        </dd>
        <dt class="col-sm-2">
            Apellido
        </dt>
        <dd class="col-sm-10">
            @Model.apellido
        </dd>
        <dt class="col-sm-2">
            Username
        </dt>
        <dd class="col-sm-10">
            @Model.username
        </dd>
        <dt class="col-sm-2">
            FechaCreacion
        </dt>
        <dd class="col-sm-10">
            @Model.fechaCreacion
        </dd>
        <dt class="col-sm-2">
            FechaModificacion
        </dt>
        <dd class="col-sm-10">
            @Model.fechaModificacion
        </dd>
        <dt class="col-sm-2">
            UsuarioModificacion
        </dt>
        <dd class="col-sm-10">
            @Model.usuarioModificacion
        </dd>
        <dt class="col-sm-2">
            Estatus
        </dt>
        <dd class="col-sm-10">
            @Model.estatus
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { id = Model.clienteId }) |
    <a asp-action="Index">Back to List</a>
</div>
