@model IEnumerable<SGCP.Web.Models.ModuloUsuarios.AdministradorModels.AdminGetModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Administradores</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                AdminId
            </th>
            <th>
                Nombre
            </th>
            <th>
                Apellido
            </th>
            <th>
                Username
            </th>
            <th>
                FechaCreacion
            </th>
            <th>
                FechaModificacion
            </th>
            <th>
                UsuarioModificacion
            </th>
            <th>
                Estatus
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.adminId
            </td>
            <td>
                @item.nombre
            </td>
            <td>
                @item.apellido
            </td>
            <td>
                @item.username
            </td>
            <td>
                @item.fechaCreacion
            </td>
            <td>
                @item.fechaModificacion
            </td>
            <td>
                @item.usuarioModificacion
            </td>
            <td>
                @item.estatus
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.adminId }) |
                @Html.ActionLink("Details", "Details", new {  id=item.adminId }) |
            </td>
        </tr>
}
    </tbody>
</table>
